0 := \fx. x
1 := \fx. f x
2 := \fx. f (f x)
3 := \fx. f (f (f x))
4 := \fx. f (f (f (f x)))
5 := \fx. f (f (f (f (f x))))
6 := \fx. f (f (f (f (f (f x)))))
7 := \fx. f (f (f (f (f (f (f x))))))
8 := \fx. f (f (f (f (f (f (f (f x)))))))
9 := \fx. f (f (f (f (f (f (f (f (f x))))))))
# ...

# Operators

PLUS := \mn. (\fx. m f (n f x))

MULT := \mn. (\fx. m (n f) x)

EXP  := \mn. n m

# Examples

PLUS 2 3

MULT 2 4

EXP 2 5